<fieldset>
<div class="field-large">
    <label for="resource">Resource: </label>
    <input type="text" name="resource" value="<%= model.get('resource') %>" />
</div>
<div class="field-large">
    <label for="user">User: </label>
    <input type="text" name="user" value="<%= model.get('user') %>" />
</div>
<div class="field-large">
    <label for="command">Command: </label>
    <textarea name="command" rows="5"><%= model.get('command') %></textarea>
</div>

<% if (model.get('_type') == "PluginHistory") { %>
    <div class="field-large">
        <label for="return_code">Return Code: </label>
        <input type="text" name="return_code" value="<%= model.get('return_code') %>" />
    </div>
<% } %>
</fieldset>